local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Samurai Hub", "DarkTheme")

--MAIN
local Main = Window:NewTab("Scripts")
local MainSection = Main:NewSection("Scripts!!!")


MainSection:NewButton("Shindo LIfe Script ", "A Shindo Life Script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/SxnwDev/Premier/main/Free-Premier.lua", true))()
end)


MainSection:NewButton("DaHood Script 1", "Must Join There Roblox Server", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/spacexrandom/Lua/main/DaHood", true))()
end)


MainSection:NewButton("DaHood Script 2", "Dahood Script 2", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/compwnter/destiny/main/loader'))();
end)


